generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Lead {
  id        Int      @id @default(autoincrement())
  name      String
  email     String   @unique
  phone     String
  notes     String?
  createdAt DateTime @default(now())
}
